{
  "name": "frontend_ta_eos",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@amcharts/amcharts5": "^5.0.19",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@material-ui/core": "^4.11.4",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "classnames": "^2.3.1",
    "http-proxy-middleware": "^2.0.4",
    "react": "^17.0.2",
    "react-countdown-circle-timer": "^3.0.8",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.1.2",
    "use-clippy": "^1.0.8",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react-app-rewire-alias": "^1.1.5",
    "react-app-rewired": "^2.1.8",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "PORT=3000 HOST=127.0.0.1 react-scripts start",
    "build": "PORT=3000 BUILD_PATH='../dist/app' react-app-rewired build",
    "test": "react-app-rewired test",
    "lint-fix": "./node_modules/react-scripts/node_modules/.bin/eslint src --fix",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "google",
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-mixed-spaces-and-tabs": [
        "error",
        "smart-tabs"
      ],
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "warn",
      "no-duplicate-imports": "error",
      "no-var": "error",
      "no-redeclare": "error",
      "no-unused-vars": "off",
      "require-jsdoc": "off",
      "no-multi-spaces": "off",
      "key-spacing": [
        "error",
        {
          "beforeColon": false,
          "afterColon": true,
          "mode": "minimum"
        }
      ],
      "max-len": [
        "error",
        120
      ],
      "no-undef": "off",
      "new-cap": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-empty-function": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/no-unused-vars": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "comma-dangle": "off",
      "react/prop-types": "off",
      "react/no-children-prop": "off",
      "no-invalid-this": "off",
      "quote-props": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
